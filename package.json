{
  "name": "kefir",
  "version": "3.7.1",
  "description": "Reactive Programming library for JavaScript inspired by Bacon.js and RxJS with focus on high performance and low memory usage",
  "main": "dist/kefir.js",
  "scripts": {
    "build-js": "grunt rollup:dev && grunt rollup:prod && cp kefir.js.flow dist/",
    "build-docs": "grunt jade:docs",
    "clean": "rm -r dist index.html || true",
    "build": "npm run clean && npm run build-js && npm run build-docs",
    "test": "grunt rollup:dev && jasmine-node --coffee --matchall test/specs && flow check",
    "test-only": "grunt rollup:dev && jasmine-node --coffee --matchall test/specs"
  },
  "keywords": [
    "frp",
    "bacon",
    "bacon.js",
    "kefir",
    "kefir.js",
    "functional",
    "reactive",
    "stream",
    "streams",
    "EventStream",
    "Rx",
    "RxJs",
    "Observable"
  ],
  "author": "Roman Pominov <rpominov@gmail.com>",
  "homepage": "https://github.com/rpominov/kefir",
  "repository": {
    "type": "git",
    "url": "http://github.com/rpominov/kefir.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@reactivex/rxjs": "5.0.0-alpha.1",
    "babel-core": "6.10.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-preset-es2015-loose-rollup": "7.0.0",
    "coffee-script": "1.10.0",
    "flow-bin": "^0.36.0",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-jade": "0.15.0",
    "grunt-rollup": "0.7.1",
    "inquirer": "0.10.1",
    "jasmine-node": "1.14.5",
    "load-grunt-tasks": "3.3.0",
    "rollup": "0.33.0",
    "rollup-plugin-babel": "2.6.1",
    "rollup-plugin-commonjs": "3.1.0",
    "rollup-plugin-node-resolve": "1.7.1",
    "rollup-plugin-uglify": "1.0.1",
    "semver": "5.0.3",
    "shelljs": "0.5.3",
    "sinon": "1.17.1",
    "time-grunt": "1.2.1",
    "transducers-js": "0.4.174",
    "transducers.js": "0.3.2",
    "zen-observable": "0.3.0"
  },
  "dependencies": {
    "symbol-observable": "^1.0.1"
  }
}
